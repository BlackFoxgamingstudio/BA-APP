ComboBox1.List = Array("Number of Datacenter locations", "Number of Servers (per location)", "number of database servers", "number oftest servers", "LOB apps", "Custom Application", "Web Applications", "Number of none virtualized Servers", "Type of hypervisor (VMware/hyper-v)", "security Regulation", "Banking Regulation", "Business Rules", "SLA's", "Current State Archetecute", "Disaster Recovery Plan", "Application Monitoring", "Application Statistics", "External File Storage", "Application Backups", "Current State Archetecute", "Disaster Recovery Plan", "Application Monitoring", "Application Statistics", "External File Storage", "Application Backups", "Email Addresses/SMTP Server", "Future State Archetecutre")
ComboBox21.List = Array("Instance Name(s)", "DNS entry", "Shared (Y / N)", "CPU Utilization", "Memory Utilization (in MB)", "Storage Utilization", "Bandwidth Utilization", "Packages / APIs / Plugins / Tools (including versions),if any", "Number of instances")
ComboBox22.List = Array("Number of instances", "Instance Name(s)", "DNS entry", "Shared (Y / N)", "CPU Utilization", "Memory Utilization (in MB)", "Storage Utilization", "Bandwidth Utilization", "Packages / APIs / Plugins / Tools (including versions),if any", "Load balancer name", "Access protocol", "Access port", "Alternate protocol", "Alternate port", "Downtime permitted (Y / N)", "Downtime period / schedule", "Maximum permitted downtime", "Scalability requirements", "Backup & Archival requirements")
ComboBox23.List = Array("Instance Name(s)", "DNS entry", "Shared (Y / N)", "CPU Utilization", "Memory Utilization (in MB)", "Storage Utilization", "Bandwidth Utilization", "Packages / APIs / Plugins / Tools (including versions),if any", "Number of instances")
ComboBox40.List = Array("Number of instances", "Instance Name(s)", "DNS entry", "Shared (Y / N)", "CPU Utilization", "Memory Utilization (in MB)", "Storage Utilization", "Bandwidth Utilization", "Packages / APIs / Plugins / Tools (including versions),if any", "Load balancer name", "Access protocol", "Access port", "Alternate protocol", "Alternate port")
With ComboBox41
.AddItem "Business / Sub-Business Area"
.AddItem "Countries/Regions serviced by Application"
.AddItem "Application Type"
.AddItem "Life Expectancy "
.AddItem "Application Age (year)"
.AddItem "Business Criticality"
.AddItem "Application Usage"
.AddItem "Application Stability"
