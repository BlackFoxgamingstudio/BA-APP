.AddItem "Number of Incidents in last 12 months"
.AddItem "FTE Count - Overall"
.AddItem "Codebase"
.AddItem "Dependencies"
.AddItem "Config"
.AddItem "Backing services"
.AddItem "Build, release, run"
.AddItem "Processes"
.AddItem "Port binding"
.AddItem "Concurrency"
.AddItem "Disposability"
.AddItem "Logs"
.AddItem "Admin processes"
.AddItem "Business Criticality"
.AddItem "Public Static Site"
.AddItem "Application Usage"
.AddItem "Application Usage (% Region wise) "
.AddItem "Countries/Regions serviced by Application"
.AddItem " Geographic distribution of Users"
End With

